<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yan.helloword.mapper.UserLoginMapper">
    <select id="queryAllUser" resultType="com.yan.helloword.Pojo.User">
        select * from user
    </select>

    <select id="findUserById" resultType="com.yan.helloword.Pojo.User">
        select * from user where username=#{username}
    </select>



</mapper>
